<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
			 d:DesignWidth="1850" d:DesignHeight="1080"
			 FontFamily="Roboto"
             x:Class="DnDigital_1e.Views.CharacterTab">
	<StackPanel Orientation="Vertical" HorizontalAlignment="Center" Margin="15" Spacing="15">
		<Grid ColumnDefinitions="*, auto, *" HorizontalAlignment="Center">
			<Border Width="1000" Height="60" Background="#20232A"/>
		</Grid>
		<StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Spacing="10">
			<Border Background="#20232A" Width="357" Height="965"/>
			<StackPanel Orientation="Vertical" Spacing="10">
				<Border Background="#20232A" Width="357" Height="637"/>
				<Border Background="#20232A" Width="357" Height="318"/>
			</StackPanel>
			<Border Background="#20232A" Width="724" Height="965"/>
			<Border Background="#20232A" Width="357" Height="965" IsVisible="False"/>
		</StackPanel>
	</StackPanel>
</UserControl>
